{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ConnectionStrings": {
        "DefaultConnection": "Server=serverNAME;Database=dbNAME;TrustServerCertificate=True;Integrated Security=True"
    },
    "Jwt": {
        "Key": "ANAHTAR_EKLEYIN", // Buraya güçlü bir anahtar ekle
        "Issuer": "http://yourdomain.com", // İsteğe bağlı, kendi domain adresini yazabilirsin
        "Audience": "http://yourdomain.com", // İsteğe bağlı, kendi domain adresini yazabilirsin
        "ExpiresInMinutes": 60
    },
    "AllowedHosts": "*"
}
